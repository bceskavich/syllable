{
  "name": "syllable",
  "version": "0.0.1",
  "description": "Responsive, interactive syllabi for the modern web.",
  "scripts": {
    "clean": "rm -rf public/*",
    "lint": "eslint src",
    "dev": "npm run clean && nodemon --exec babel-node --stage 0 -- dev-server.js",
    "build": "npm run clean && webpack --progress --colors --config config/webpack.production.js",
    "deploy": "NODE_ENV=production npm run build && surge --project ./build --domain syllable.surge.sh"
  },
  "author": "Billy Ceskavich",
  "dependencies": {
    "alt": "^0.17.1",
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.23",
    "babel-loader": "^5.3.1",
    "babel-relay-plugin": "^0.2.2",
    "config": "^1.15.0",
    "css-loader": "^0.15.2",
    "css-patterns": "^0.2.0",
    "express": "^4.13.1",
    "express-graphql": "^0.3.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "graphql": "^0.4.3",
    "graphql-relay": "^0.3.2",
    "html-webpack-plugin": "^1.6.0",
    "lodash": "^3.10.1",
    "node-sass": "^3.2.0",
    "nodemon": "^1.3.7",
    "proxy-middleware": "^0.13.1",
    "react": "^0.14.0-beta3",
    "react-dom": "^0.14.0-beta3",
    "react-hot-loader": "^1.2.8",
    "react-relay": "^0.2.1",
    "react-router": "^1.0.0-beta3",
    "react-router-relay": "^0.4.4",
    "react-tools": "^0.13.3",
    "request": "^2.58.0",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1",
    "webpack-config-merger": "0.0.3",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "eslint": "^1.0.0-rc-1",
    "babel-eslint": "^3.1.23",
    "eslint-plugin-react": "^2.7.1"
  }
}
